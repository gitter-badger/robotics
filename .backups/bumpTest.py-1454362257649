from bot_utilitySensor import *

import brickpi
import time

interface=brickpi.Interface()
motors = [2, 3]
initInterface(interface, motors)

for i in range(1):
    goStraight(40, interface, motors)
    turn(-90, interface, motors)
    goStraight(40, interface, motors)
    turn(-90, interface, motors)
    goStraight(40, interface, motors)
    turn(-90, interface, motors)
    goStraight(40, interface, motors)
    turn(-90, interface, motors)

interface.terminate()
